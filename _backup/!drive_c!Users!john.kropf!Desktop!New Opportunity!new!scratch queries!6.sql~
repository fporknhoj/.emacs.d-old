SELECT 
 ([Link TPS Opportunity].OPP_NBR) AS OPP_NBR,
 trim([Link TPS Opportunity].ORIGINAL_SOURCE_CODE) AS ORIGINAL_SOURCE_CODE,
 trim([Link TPS Opportunity].IS_ACTIVE) AS IS_ACTIVE,
 trim([Link TPS Opportunity].HAS_MANUAL) AS HAS_MANUAL,
 trim([Link TPS Opportunity].HAS_BID) AS HAS_BID,
 trim([Link TPS Opportunity].HAS_PRICE) AS HAS_PRICE,
 trim([Link TPS Opportunity].HAS_MOVES) AS HAS_MOVES,
 ([Link TPS Opportunity].CUST_NBR) AS CUST_NBR,
 trim([Link TPS Opportunity].CUST_NAME) AS CUST_NAME,
 ([Link TPS Opportunity].PARENT_CUST_NBR) AS PARENT_CUST_NBR,
 trim([Link TPS Opportunity].PARENT_CUST_NAME) AS PARENT_CUST_NAME,
 ([Link TPS Opportunity].REPORTING_CUST_NBR) AS REPORTING_CUST_NBR,
 trim([Link TPS Opportunity].REPORTING_CUST_NAME) AS REPORTING_CUST_NAME,
 ([Link TPS Opportunity].BENE_CUST_NBR) AS BENE_CUST_NBR,
 trim([Link TPS Opportunity].BENE_CUST_NAME) AS BENE_CUST_NAME,
 trim([Link TPS Opportunity].PROSPECT_NAME) AS PROSPECT_NAME,
 trim([Link TPS Opportunity].SOURCE_REP) AS SOURCE_REP,
 trim([Link TPS Opportunity].SOURCE_REP_NAME) AS SOURCE_REP_NAME,
 trim([Link TPS Opportunity].ORIGIN_CITY) AS ORIGIN_CITY,
 trim([Link TPS Opportunity].ORIGIN_STATE) AS ORIGIN_STATE,
 trim([Link TPS Opportunity].ORIGIN_ZIP3) AS ORIGIN_ZIP3,
 trim([Link TPS Opportunity].ORIGIN_MARKET_RAMP) AS ORIGIN_MARKET_RAMP,
 trim([Link TPS Opportunity].ORIGIN_RAMP) AS ORIGIN_RAMP,
 trim([Link TPS Opportunity].ORIGIN_ZONE) AS ORIGIN_ZONE,
 trim([Link TPS Opportunity].DEST_CITY) AS DEST_CITY,
 trim([Link TPS Opportunity].DEST_STATE) AS DEST_STATE,
 trim([Link TPS Opportunity].DEST_ZIP3) AS DEST_ZIP3,
 trim([Link TPS Opportunity].DEST_MARKET_RAMP) AS DEST_MARKET_RAMP,
 trim([Link TPS Opportunity].DEST_RAMP) AS DEST_RAMP,
 trim([Link TPS Opportunity].DEST_ZONE) AS DEST_ZONE,
 trim([Link TPS Opportunity].MOST_RECENT_SOURCE) AS MOST_RECENT_SOURCE,
 ([Link TPS Opportunity].MOST_RECENT_DATE) AS MOST_RECENT_DATE,
 trim([Link TPS Opportunity].MOST_RECENT_SALESREP) AS MOST_RECENT_SALESREP,
 trim([Link TPS Opportunity].MOST_RECENT_EQUIPTYPE) AS MOST_RECENT_EQUIPTYPE,
 ([Link TPS Opportunity].MOST_RECENT_EQUIPLEN) AS MOST_RECENT_EQUIPLEN,
 ([Link TPS Opportunity].MOST_RECENT_VOLUME) AS MOST_RECENT_VOLUME,
 trim([Link TPS Opportunity].MOST_RECENT_VOLUME_TYPE) AS MOST_RECENT_VOLUME_TYPE,
 ([Link TPS Opportunity].MOST_RECENT_MILES) AS MOST_RECENT_MILES,
 ([Link TPS Opportunity].MOST_RECENT_RPM) AS MOST_RECENT_RPM,
 trim([Link TPS Opportunity].MOST_RECENT_MODE) AS MOST_RECENT_MODE,
 trim([Link TPS Opportunity].MOST_RECENT_SUBMODE) AS MOST_RECENT_SUBMODE,
 ([Link TPS Opportunity].IML_PRICE_NBR) AS IML_PRICE_NBR,
 ([Link TPS Opportunity].IML_PRICE_SEQ) AS IML_PRICE_SEQ,
 ([Link TPS Opportunity].IML_PRICE_AMT) AS IML_PRICE_AMT,
 ([Link TPS Opportunity].IML_PRICE_VOLUME) AS IML_PRICE_VOLUME,
 trim([Link TPS Opportunity].IML_PRICE_VOLUME_TYPE) AS IML_PRICE_VOLUME_TYPE,
 ([Link TPS Opportunity].IML_PRICE_EFF_DATE) AS IML_PRICE_EFF_DATE,
 ([Link TPS Opportunity].IML_PRICE_EXP_DATE) AS IML_PRICE_EXP_DATE,
 trim([Link TPS Opportunity].IML_PRICE_STATUS) AS IML_PRICE_STATUS,
 trim([Link TPS Opportunity].IML_PRICE_SALESREP) AS IML_PRICE_SALESREP,
 ([Link TPS Opportunity].IML_PRICE_MILES) AS IML_PRICE_MILES,
 ([Link TPS Opportunity].OTR_PRICE_NBR) AS OTR_PRICE_NBR,
 ([Link TPS Opportunity].OTR_PRICE_SEQ) AS OTR_PRICE_SEQ,
 ([Link TPS Opportunity].OTR_PRICE_AMT) AS OTR_PRICE_AMT,
 ([Link TPS Opportunity].OTR_PRICE_VOLUME) AS OTR_PRICE_VOLUME,
 trim([Link TPS Opportunity].OTR_PRICE_VOLUME_TYPE) AS OTR_PRICE_VOLUME_TYPE,
 ([Link TPS Opportunity].OTR_PRICE_EFF_DATE) AS OTR_PRICE_EFF_DATE,
 ([Link TPS Opportunity].OTR_PRICE_EXP_DATE) AS OTR_PRICE_EXP_DATE,
 trim([Link TPS Opportunity].OTR_PRICE_STATUS) AS OTR_PRICE_STATUS,
 trim([Link TPS Opportunity].OTR_PRICE_SALESREP) AS OTR_PRICE_SALESREP,
 ([Link TPS Opportunity].OTR_PRICE_MILES) AS OTR_PRICE_MILES,
 ([Link TPS Opportunity].IML_BID_ID) AS IML_BID_ID,
 ([Link TPS Opportunity].IML_BID_VOLUME) AS IML_BID_VOLUME,
 trim([Link TPS Opportunity].IML_BID_VOLUME_TYPE) AS IML_BID_VOLUME_TYPE,
 ([Link TPS Opportunity].IML_BID_DATE) AS IML_BID_DATE,
 trim([Link TPS Opportunity].IML_BID_SALESREP) AS IML_BID_SALESREP,
 ([Link TPS Opportunity].IML_BID_MILES) AS IML_BID_MILES,
 ([Link TPS Opportunity].OTR_BID_ID) AS OTR_BID_ID,
 ([Link TPS Opportunity].OTR_BID_VOLUME) AS OTR_BID_VOLUME,
 trim([Link TPS Opportunity].OTR_BID_VOLUME_TYPE) AS OTR_BID_VOLUME_TYPE,
 ([Link TPS Opportunity].OTR_BID_DATE) AS OTR_BID_DATE,
 trim([Link TPS Opportunity].OTR_BID_SALESREP) AS OTR_BID_SALESREP,
 ([Link TPS Opportunity].OTR_BID_MILES) AS OTR_BID_MILES,
 ([Link TPS Opportunity].LAST_SHIP_DATE) AS LAST_SHIP_DATE,
 ([Link TPS Opportunity].OTR_SHIP_VOLUME_ANNUAL) AS OTR_SHIP_VOLUME_ANNUAL,
 ([Link TPS Opportunity].OTR_LAST_SHIP_DATE) AS OTR_LAST_SHIP_DATE,
 trim([Link TPS Opportunity].OTR_MOVE_SALESREP) AS OTR_MOVE_SALESREP,
 ([Link TPS Opportunity].IML_SHIP_VOLUME_ANNUAL) AS IML_SHIP_VOLUME_ANNUAL,
 ([Link TPS Opportunity].IML_LAST_SHIP_DATE) AS IML_LAST_SHIP_DATE,
 trim([Link TPS Opportunity].IML_MOVE_SALESREP) AS IML_MOVE_SALESREP,
 trim([Link TPS Opportunity].CREATE_USER) AS CREATE_USER,
 trim([Link TPS Opportunity].CREATE_PROGRAM) AS CREATE_PROGRAM,
 ([Link TPS Opportunity].CREATE_TIMESTAMP) AS CREATE_TIMESTAMP,
 trim([Link TPS Opportunity].LAST_USER) AS LAST_USER,
 trim([Link TPS Opportunity].LAST_PROGRAM) AS LAST_PROGRAM,
 ([Link TPS Opportunity].LAST_TIMESTAMP) AS LAST_TIMESTAMP,
 trim([Link TPS Opportunity].WHOLESALE_FLAG) AS WHOLESALE_FLAG 
INTO [New Op]
FROM [Link TPS Opportunity]
WHERE opp_nbr > (SELECT max(opp_nbr) FROM base) AND CUST_NBR NOT IN (1494, 39175, 39174, 39176, 39177, 39173);
